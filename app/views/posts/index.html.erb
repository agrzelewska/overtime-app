<h1>Posts</h1>
<table class="table table-striped table-hover">
  <head>
    <tr>
      <th>Date</th>
      <th>Overtime</th>
      <th class="hidden-xs">Rationale</th>
      <th>Status</th>
      <th></th>
    </tr>
  </head>
  <tbody id="posts">
  <%= render @posts %>
  </tbody>
</table>

<div id="paginator"
  <%= paginate @posts, remote: true %>
</div>
