<h1>Audit Logs Dashboard</h1>
<table class="table table-striped table-hover">
  <head>
    <tr>
      <th>#</th>
      <th>Employee</th>
      <th>Week Starting</th>
      <th>Confirmed At</th>
      <th>Status</th>
    </tr>
  </head>
  <tbody id="audit_logs">
  <%= render 'audit_log' %>
  </tbody>
</table>

<div id='paginator'>
  <%= paginate @audit_logs, remote: true %>
</div>
